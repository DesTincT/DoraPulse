{
  email ops@example.com
}

dora.example.com {
  encode zstd gzip

  @webappAssets {
    path /webapp/assets/*
  }
  header @webappAssets Cache-Control "public, max-age=31536000, immutable"

  @webappHtml {
    path /webapp /webapp/
  }
  header @webappHtml Cache-Control "no-cache"

  reverse_proxy localhost:8080
}
